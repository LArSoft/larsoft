# build everything that does not depend on art

basequal=${1}

#require_buildfw_version 6.00.00 || return
  if version_greater \
   6.00.00     v$(print_version | sed -e 's&^.*[ \t]\{1,\}&&' -e 's&\.&_&g' ); then
    echo "Need buildFW 6.00.00 or better." 1>&2
    return 1
  fi

# specify version to bootstrap and setup ups
check_ups @UPS_VERSION@

# infrastructure
for cv in @CMAKE_VERSION_LIST@; do
  do_build cmake ${cv}
done

# Compilers.
bf_build_compilers() {
  # Attempt to pull required_items.
  (( bf_build_base )) || ! maybe_pull_gcc && \\
    { local no_binary_download=1
      maybe_build_gcc; }
  bf_build_cmake
  (( bf_build_base )) || ! maybe_pull_other_compilers && \\
    { local no_binary_download=1
      maybe_build_other_compilers v3_40_01_00 \\
      v3_9_15; }
}
bf_build_compilers && unset bf_build_base bf_build_compilers bf_handle_cmake

build_noarch eigen @EIGEN_VERSION@
do_build sundials @SUNDIALS_VERSION@
do_build stan_math @STAN_MATH_VERSION@
do_build stan @STAN_VERSION@

return 0

