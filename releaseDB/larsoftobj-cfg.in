# build larsoftobj distribution

basequal=${1}

# specify version to bootstrap and setup ups
check_ups v6_0_8

build_bundle gallery-@GALLERY_DOT_VERSION@ ${basequal}
status=$?
if (( ${status} != 0)); then
  cat 1>&2 <<EOF
Build of gallery-@GALLERY_DOT_VERSION@ bundle failed with status ${status}
EOF
  return ${status}
fi
[[ "${build_label}" == py3 ]] && pyqual=:${build_label}

build_noarch mrb @MRB_VERSION@

# only needed because we don't have a nutools build
do_build dk2nudata @DK2NUDATA_VERSION@ ${basequal}${pyqual}

build_cet nusimdata  @NUSIMDATA_VERSION@ ${basequal}${pyqual}
build_cet larcoreobj @LARCOREOBJ_VERSION@ ${basequal}${pyqual}
build_cet -n larcorealg @LARCOREALG_VERSION@ ${basequal}${pyqual}
build_cet lardataobj @LARDATAOBJ_VERSION@ ${basequal}${pyqual}
build_cet lardataalg @LARDATAALG_VERSION@ ${basequal}${pyqual}
build_cet larsoftobj @LARSOFTOBJ_VERSION@ ${basequal}${pyqual}

return 0
